{
  "name": "grouper-media-client",
  "version": "0.1.3",
  "private": false,
  "main": "src/index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target wc --name media-component src/App.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "kurento-utils": "^6.13.1",
    "lodash": "^4.17.15",
    "lodash-uuid": "0.0.3",
    "socket.io-client": "^2.3.0",
